# =============================================================================
# Multi-Agent System .gitignore
# =============================================================================

# Compressed files
*.tar.gz
*.zip

# OS files
.DS_Store
Thumbs.db

# Editor files
*.tmp
*.swp
*.swo
*~
.idea/
.vscode/
*.sublime-*

# =============================================================================
# Node.js
# =============================================================================
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.npm
.yarn-integrity

# =============================================================================
# Workspace (Runtime files)
# =============================================================================

# Log files
workspace/logs/*

# Signal files (IPC)
workspace/signals/*

# Status files
workspace/status/*

# Artifacts (intermediate outputs)
workspace/artifacts/*

# User input files
workspace/input/*

# Task files
workspace/tasks/**/*.json

# Output files (completed projects)
workspace/output/*

# Project files (temporary workspace)
workspace/project/*

# Review files
workspace/reviews/*

# State files
workspace/state/*

# Callback files
workspace/callbacks/*

# =============================================================================
# Dashboard
# =============================================================================
packages/dashboard/.dashboard.pids
packages/dashboard/node_modules/

# =============================================================================
# Keep empty directories
# =============================================================================
!workspace/logs/.gitkeep
!workspace/signals/.gitkeep
!workspace/status/.gitkeep
!workspace/artifacts/.gitkeep
!workspace/input/.gitkeep
!workspace/output/.gitkeep
!workspace/project/.gitkeep
!workspace/state/.gitkeep
!workspace/reviews/.gitkeep
!workspace/callbacks/.gitkeep
!workspace/tasks/.gitkeep
